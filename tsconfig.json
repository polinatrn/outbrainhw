{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "inlineSourceMap": true,
      "declaration": false,
      "noImplicitAny": false,
      "outDir": "tmp",
      "skipLibCheck": true
    },
    "exclude": [
      "node_modules",
    ],
    "typeRoots": [
      "./node_modules/@types/"
    ]
  }